{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Company"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "email": {
      "type": "email"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "company"
    },
    "logo": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "interviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::interview.interview",
      "mappedBy": "company"
    },
    "salaries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::salary.salary",
      "mappedBy": "company"
    }
  }
}
